        $check['{{entity}}'] = new Varien_Object(array(
            'prefix'        => Mage::getStoreConfig('{{namespace}}_{{module}}/{{entity}}/url_prefix'),
            'suffix'        => Mage::getStoreConfig('{{namespace}}_{{module}}/{{entity}}/url_suffix'),
            'model'         =>'{{namespace}}_{{module}}/{{entity}}',
            'controller'    => '{{entity}}',
            'action'        => 'view',
            'param'         => 'id',
            'check_path'    => {{isTree}}
        ));
